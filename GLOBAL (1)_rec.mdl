	0.37
	~	
	~		|

tasa de eliminacion de publicaciones=
	0.23
	~	
	~		|

Tasa de inactividad=
	0.17
	~	
	~		|

Tasa de publicacion no paga=
	0.52
	~	
	~		|

tasa de publicacion paga=
	0.48
	~	
	~		|

tasa de usuarios nuevos=
	0.45
	~	
	~		|

Usuarios Activos= INTEG (
	Nuevos Usuarios-Usuarios convertidos-Usuarios inactivos,
		50000)
	~	
	~		|

Usuarios convertidos=
	Usuarios Activos*tasa de conversión
	~	
	~		|

Usuarios inactivos=
	Tasa de inactividad*Usuarios Activos
	~	
	~		|

Visualizacion por publicidad=
	((Valor por publicidad-25)/(1000-25))*Usuarios Activos
	~	
	~		|

