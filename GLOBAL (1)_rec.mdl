tasa de visualizacion=
	RANDOM UNIFORM(0.01, 1, 0.2)
	~	
	~		|

Contenido monetizable=
	Empresa*tasa de publicacion paga
	~	
	~		|

Contenido no monetizado=
	Usuarios Activos*Tasa de publicacion no paga
	~	
	~		|

Valor por publicidad=
	IF THEN ELSE(Time<3, RANDOM UNIFORM(50, 1000, 50), IF THEN ELSE(Time<7, RANDOM UNIFORM\
		(25, 1000, 25), IF THEN ELSE(Time<10, RANDOM UNIFORM(50, 1000, 50 ), RANDOM UNIFORM\
		(25, 1000, 25))))
	~	
	~		|

Visualizacion normal=
	tasa de visualizacion*Usuarios Activos
	~	
	~		|

Contenido Eliminado=
	Contenido total*tasa de eliminacion de publicaciones
	~	
	~		|

Contenido total= INTEG (
	Contenido monetizable+Contenido no monetizado-Contenido Eliminado,
		100000)
	~	
	~		|

Costo fijo=
	800000
	~	
	~		|

Empresa= INTEG (
	Usuarios convertidos,
		200000)
	~	
	~		|

Gastos=
	Costo fijo+Costo variable
	~	
	~		|

ingreso por publicidad=
	Contenido monetizable*Valor por publicidad
	~	
	~		|

Ingreso Total= INTEG (
	ingresos extra+ingreso por publicidad-Gastos,
		900000)
	~	
	~		|

ingresos extra=
	RANDOM UNIFORM( 1e+06, 5e+06, 100000)
	~	
	~		|

Nuevos Usuarios=
	Usuarios Activos*tasa de usuarios nuevos
	~	
	~		|

